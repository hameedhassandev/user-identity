@model IEnumerable<IdentityRole>

@{
    ViewData["Title"] = "Role";
}

<h1>Roles</h1>
<partial name="_RoleForm" model="new RoleViewModel()" />

<table class="table table-striped mt-3">
    <thead>
        <tr class="bg-success text-white">
            <th>Id</th>
            <th>Role Name</th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
           
        </tr>
        }
    </tbody>
</table>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
